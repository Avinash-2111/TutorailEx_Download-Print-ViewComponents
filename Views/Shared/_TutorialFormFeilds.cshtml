@model Tutorial
<div class="form-group">
    <lable asp-for="Id" hidden></lable>
    <input type="text" hidden asp-for="Id" id="Tutorial_Id" />

</div>
<div class="form-group">
    <lable asp-for="TutorialName">Name Of The Tutorial</lable>
    <input type="text" asp-for="TutorialName" class="form-control" id="Tutorial_Name" />
    <span asp-validation-for="TutorialName" class="text-danger"></span>
</div>
<div class="form-group">
    <lable asp-for="TutorialDescription">Tutorial Description</lable>
    <input type="text" asp-for="TutorialDescription" class="form-control" id="Tutorial_Description" />
    <span asp-validation-for="TutorialDescription" class="text-danger"></span>
</div>


